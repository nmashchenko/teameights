{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["migration:run", ""^build"],
      "outputs": ["build/**", "dist/**"]
    },
    "test": {
      "dependsOn": ["migration;run", "build"],
      "outputs": ["coverage/**"],
      "inputs": ["src/**/*.tsx", "src/**/*.ts"]
    },
    "lint": {
     "dependsOn": ["^lint"]
    },
    "dev": {
      "cache": false,
      "dependsOn": ["migration:run", "storybook"]
    }
  }
}
