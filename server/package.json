{
  "name": "server",
  "version": "0.0.1",
  "description": "This server is a backend for team8s written in NestJS",
  "author": "Nikita Mashchenko",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "cross-env NODE_ENV=production nest start",
    "start:dev": "cross-env NODE_ENV=dev nest start --watch",
    "start:full": "concurrently \" cross-env NODE_ENV=dev nest start --watch \" \" npm run client \"",
    "start:debug": "cross-env NODE_ENV=dev nest start --debug --watch",
    "start:prod": "node dist/main",
    "client": "npm run start --prefix ../client",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "cross-env NODE_ENV=dev jest",
    "test:watch": "cross-env NODE_ENV=dev jest --watch",
    "test:cov": "cross-env NODE_ENV=dev jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@faker-js/faker": "^7.6.0",
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/mongoose": "^9.2.1",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/platform-socket.io": "^9.1.6",
    "@nestjs/serve-static": "^3.0.0",
    "@nestjs/swagger": "^6.1.4",
    "@nestjs/websockets": "^9.1.6",
    "@types/lodash": "^4.14.191",
    "@types/qs": "^6.9.7",
    "bcryptjs": "^2.4.3",
    "check-password-strength": "^2.0.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "concurrently": "^7.6.0",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "google-auth-library": "^8.7.0",
    "lodash": "^4.17.21",
    "mongoose": "^6.8.1",
    "nestjs-asyncapi": "^1.1.1",
    "nodemailer": "^6.8.0",
    "pug": "^3.0.2",
    "qs": "^6.11.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "socket.io-client": "^4.6.1",
    "swagger-ui-express": "^4.6.0",
    "uuidv4": "^6.2.13"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.8",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "jest": "28.1.3",
    "mongodb-memory-server": "^8.11.5",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testTimeout": 20000,
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/../src/$1"
    },
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
